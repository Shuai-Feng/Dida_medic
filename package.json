{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@types/classnames": "^2.2.10",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.18",
    "animate.css": "^4.1.1",
    "antd": "^3.26.18",
    "axios": "^0.20.0",
    "classnames": "^2.2.6",
    "lint-staged": "^10.0.7",
    "pexels": "^1.0.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "swiper": "^3.4.2",
    "umi": "^3.2.18",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^6.0.1"
  }
}
